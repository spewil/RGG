# generating lots of graphs and saving their data 
# generate the dictionaries of a bunch of graphs 
# loading the data to analyze 

d = net.Dictionary()

# Writing JSON data
with open(str(net.Param_String)+'.json', 'w') as f:
     json.dumps(net.__dict__, f)

# Reading data back
with open('data.json', 'r') as f:
     data = json.load(f)

class Foo(object):
    def __init__(self):
        self.x = 1
        self.y = 2

foo = Foo()
s = json.dumps(foo) # raises TypeError with "is not JSON serializable"

s = json.dumps(foo.__dict__) # s set to: {"x":1, "y":2}